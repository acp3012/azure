{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "coviddataingestion-adf"
		},
		"ls_ablob_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_storage'"
		},
		"ls_adl_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adl_sink'"
		},
		"ls_adl_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covid19adlstorageact.dfs.core.windows.net/"
		},
		"ls_ecdc_data_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseurl}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "coviddataingestion-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_storage_connectionString": {
						"value": "[parameters('ls_ablob_storage_connectionString')]"
					},
					"ls_adl_sink_accountKey": {
						"value": "[parameters('ls_adl_sink_accountKey')]"
					},
					"ls_adl_sink_properties_typeProperties_url": {
						"value": "[parameters('ls_adl_sink_properties_typeProperties_url')]"
					},
					"ls_ecdc_data_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_data_http_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}